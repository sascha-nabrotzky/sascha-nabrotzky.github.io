  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.12",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-source-contentful": "^2.3.35",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
